@startuml
!include ../../../KeypleCoreApi/ApplicationApi/puml/KeypleCore-Api-ClassDiag-PlantUmlSetting.iuml

package ReaderApi.high.forTicketingApplication #FFFFFF {
  !include ../../../KeypleCoreApi/ApplicationApi/puml/KeypleCore-ApplicationApi-ClassDiag-SE_Proxy-ClassElements.iuml!SEPROXY_SELECTOR_AID_ReaderAPI
  !include ../../../KeypleCoreApi/ApplicationApi/puml/KeypleCore-ApplicationApi-ClassDiag-SE_Proxy-ClassElements.iuml!SEPROXY_SELECTOR_COMPL
  hide AtrSelector
  together {
  !include ../../../KeypleCoreApi/ApplicationApi/puml/KeypleCore-ApplicationApi-ClassDiag-Selection-ClassElements.iuml!SELECTION_REQUEST_ReaderAPI
  !include ../../../KeypleCoreApi/ApplicationApi/puml/KeypleCore-ApplicationApi-ClassDiag-SE_Proxy-ClassElements.iuml!SEPROXY_SELECTOR_ReaderAPI
  !include ../../../KeypleCoreApi/ApplicationApi/puml/KeypleCore-ApplicationApi-ClassDiag-SE_Proxy-ClassElements.iuml!SEPROXY_READER_ReaderAPI
  !include ../../../KeypleCoreApi/ApplicationApi/puml/KeypleCore-ApplicationApi-ClassDiag-SE_Proxy-ClassElements.iuml!SEPROXY_CHANNEL
  }
'  AbstractMatchingSe -right[hidden]-> SeSelector
'  SeSelector -right[hidden]-> SeReader
  AidSelector +-right- FileOccurrence
  AidSelector +-left- FileControlInformation
  AidSelector "1" *-- "1" FileOccurrence
  AidSelector "1" *-- "1" FileControlInformation
  SeSelector +-up- AidSelector
  SeSelector "1" o-- "0..1" AidSelector
}

package ReaderApi.low.forSeSolutionLib #FFFFFF {

'  SeSelector -right[hidden]-> ProxyReader


  !include ../../../KeypleCoreApi/PluginApi/puml/KeypleCore-PluginApi-ClassDiag-SE_Proxy-ClassElements.iuml!MESSAGE_PROXYREADER_0_9
  together {
    !include ../../../KeypleCoreApi/PluginApi/puml/KeypleCore-PluginApi-ClassDiag-SE_Proxy-ClassElements.iuml!MESSAGE_SEREQUEST_0_9
  }
  SeSelector "0..1" <-down-o "1" SeRequest : selector
  AbstractMatchingSe "0..1" <-down-o "1" SeResponse : selector

  ProxyReader "1" -down- "+" SeRequest : sends >
  ProxyReader "1" -down- "+" SeResponse : receives >
  SeRequest -right[hidden]-> SeResponse

  together {
    !include ../../../KeypleCoreApi/PluginApi/puml/KeypleCore-PluginApi-ClassDiag-SE_Proxy-ClassElements.iuml!MESSAGE_APDUREQUEST_ReaderAPI
  }
  SeRequest "1" o-down-> "*" ApduRequest : apduRequests
  SeResponse "0..1" o-down-> "*" ApduResponse : apduResponses
  ApduRequest -right[hidden]-> ApduResponse

  !include ../../../KeypleCoreApi/PluginApi/puml/KeypleCore-PluginApi-ClassDiag-SE_Proxy-ClassElements.iuml!MESSAGE_STATUS_0_9
  SeResponse "1" o--> "0..1" SelectionStatus : selectionStatus
  SelectionStatus "1" o--> "0..1" ApduResponse : fci

  SeReader <|-down- ProxyReader : extends

  ChannelControl -down- ProxyReader

  class SeRequest <<Serializable>>
  class SeResponse <<Serializable>>
@enduml